<Project Sdk="Microsoft.NET.Sdk">

 <PropertyGroup>
   
	<TargetFramework>net6.0-windows</TargetFramework>
	<UseWPF>true</UseWPF>
	<Title>一个简易开发的工具类库包</Title>
	<Authors>liuxin</Authors>
	<Version>3.0.4</Version>
	<PackageLicenseFile>LICENSE.md</PackageLicenseFile>
	<AssemblyVersion>2.0.2</AssemblyVersion>
	<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <SuppressTfmSupportBuildWarnings>true</SuppressTfmSupportBuildWarnings>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/guchen66/IT.Tangdao.Core</RepositoryUrl>
    <PackAsTool>False</PackAsTool>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
  </PropertyGroup>

 <ItemGroup>
   <None Include="..\LICENSE.md">
     <Pack>True</Pack>
     <PackagePath>\</PackagePath>
   </None>
   <None Include="..\ReadMe.md">
     <Pack>True</Pack>
     <PackagePath>\</PackagePath>
   </None>
 </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="9.0.0">
      <PrivateAssets>all</PrivateAssets>
     <IncludeAssets>analyzers; build</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Roslynator.Analyzers" Version="4.14.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.IO.Ports" Version="9.0.8" />
	  <Reference Include="Microsoft.Windows.SDK.NET">
		  <HintPath>E:\nuget\MyPackages\Microsoft.Windows.SDK.NET.dll</HintPath>
	  </Reference>
	  <Reference Include="WinRT.Runtime">
		  <HintPath>E:\nuget\MyPackages\WinRT.Runtime.dll</HintPath>
	  </Reference>
  </ItemGroup>

</Project>
